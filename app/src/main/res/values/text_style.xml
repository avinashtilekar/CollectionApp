<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MyToolbarStyle" parent="Widget.MaterialComponents.Toolbar">
        <item name="titleTextAppearance">@style/AppFont.LabelMedium.White</item>
        <item name="titleTextColor">@android:color/white</item>
        <item name="android:background">@color/colorPrimary</item>
    </style>

    <!--region Blue-->
    <style name="AppFont.LabelSmall.Blue">
        <item name="android:textColor">@color/colorAccent</item>
    </style>

    <style name="AppFont.LabelMedium.Blue">
        <item name="android:textColor">@color/colorAccent</item>
    </style>

    <style name="AppFont.LabelLarge.Blue">
        <item name="android:textColor">@color/colorAccent</item>
    </style>

    <style name="AppFont.XLabelLarge.Blue">
        <item name="android:textColor">@color/colorAccent</item>
    </style>

    <style name="AppFont.XXLabelLarge.Blue">
        <item name="android:textColor">@color/colorAccent</item>
    </style>

    <!--endregion Blue-->

    <!--region White-->
    <style name="AppFont.LabelSmall.White">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="AppFont.LabelMedium.White">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="AppFont.LabelLarge.White">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="AppFont.XLabelLarge.White">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="AppFont.XXLabelLarge.White">
        <item name="android:textColor">@color/white</item>
    </style>
    <!--endregion White-->

</resources>